<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="org.ksu.swe6673.testinpeace.resumebuilder.FormController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TextField fx:id="firstName"/>

    <TextField fx:id="lastName"/>
    <TextField fx:id="address"/>
    <TextField fx:id="contactInfo"/>
    <TextField fx:id="email"/>
    <TextField fx:id="linkedin"/>
    <TextField fx:id="github"/>
    <TextField fx:id="phone"/>
    <TextField fx:id="workHistory"/>
    <TextField fx:id="workYears"/>
    <TextField fx:id="company"/>
    <TextField fx:id="role"/>
    <TextField fx:id="description"/>
    <TextField fx:id="education"/>
    <TextField fx:id="schoolYears"/>
    <TextField fx:id="degree"/>
    <TextField fx:id="school"/>
    <TextField fx:id="major"/>
    <TextField fx:id="gpa"/>
    <TextField fx:id="status"/>

    <Label fx:id="submitLabel" text="Not Submitted"/>
    <Button fx:id="submitButton" text="Submit" onAction="#onSubmitButtonClick"/>
</VBox>
